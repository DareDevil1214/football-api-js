{
  "version": 2,
  "builds": [
    {
      "src": "api/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/docs",
      "dest": "api/api.js"
    },
    {
      "src": "/api/docs/(.*)",
      "dest": "api/api.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/api.js"
    }
  ]
}
